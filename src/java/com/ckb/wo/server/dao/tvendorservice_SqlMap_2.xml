    <sql id="selectwithjoin">
    select tvendorservice.*,tvendor.*,
    tservicetype.servicecode,tservicetype.servicename,
    tservicemode.smodecode,tservicemode.smodename,
    tservicemodedetail.smdcode,tservicemodedetail.smdname,
    tservicemodedetail.smdtservicemode_fk ,
    torder.ordercode,torder.ordername,
    tdeliveryterm.dtcode,tdeliveryterm.dtname,
    otlocation.locationcode as originlocationcode, otlocation.locationname as originlocationname,
    dtlocation.locationcode as destinationlocationcode,dtlocation.locationname as destinationlocationname, 
    tcurrency.*
    from tvendorservice left outer join tvendor
    on tvendorservice.tvendor_fk=tvendor.tvendor_pk
    left outer join tservicetype 
    on tvendorservice.tservicetype_fk=tservicetype.tservicetype_pk
    left outer join tservicemode
    on tvendorservice.tservicemode_fk=tservicemode.tservicemode_pk
    left outer join tservicemodedetail
    on tvendorservice.tservicemodedetail_fk=tservicemodedetail.tservicemodedetail_pk
    left outer join torder 
    on tvendorservice.torder_fk=torder.torder_pk
    left outer join tdeliveryterm 
    on tvendorservice.tdeliveryterm_fk=tdeliveryterm.tdeliveryterm_pk
    left outer join tlocation otlocation 
    on tvendorservice.origintlocation_fk=otlocation.tlocation_pk
    left outer join tlocation dtlocation
    on tvendorservice.destinationtlocation_fk=dtlocation.tlocation_pk
    left outer join tcurrency
    on tvendorservice.tcurrency_fk=tcurrency.tcurrency_pk  
    </sql>
  
